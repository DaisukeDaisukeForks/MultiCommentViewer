<Window x:Class="Mcv.Core.SplashWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mcv.Core" d:DataContext="{d:DesignInstance Type=local:SplashWindowViewModel}"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="#1E1E1E">
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="135,94,0,0" TextWrapping="Wrap" Text="{Binding Logs}"
                 VerticalAlignment="Top" Width="510" Height="260" Background="#333333" Foreground="White" />

    </Grid>
</Window>
